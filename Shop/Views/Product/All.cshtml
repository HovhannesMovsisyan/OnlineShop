
@{
    var all = ViewBag.product as List<Products>;

}
    <div class="row">
        @foreach (var item in all)
        {
        <div class="col-md-4">
            <h3>@item.Name @item.Price </h3>
            <a href="/Product/AddToCart/@item.Id">Move to Cart</a>
            <a href="/Product/Details/@item.Id">Details</a>
        </div>
        }
    </div>