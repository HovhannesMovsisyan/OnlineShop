<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Shop</title>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />
    <environment include="Development">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
            
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body>
    <nav class="navbar navbar-expand-sm bg-dark">

        <!-- Links -->
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="nav-link text-white" href="/">Home</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-white" href="/User/Signup">Signup</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-white" href="/User/Profile">Profile</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-white" href="/Product/Add">Add Product</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-white" href="/Product/All">Magazine</a>
            </li>
            <li class="nav-item">
                <a class="nav-link text-white" href="/Cart/dashboard">Cart</a>
            </li>
        </ul>

    </nav>

    <partial name="_CookieConsentPartial" />
    
    <div class="container body-content">
        
        @RenderBody()
        @*<hr />*@
        
        <footer>
            <p>&copy; 2019 - Shop</p>
        </footer>
    </div>

   
    @RenderSection("Scripts", required: false)
</body>
</html>
